<div class="container">
  <h1>Client User Details</h1>
  <%= form_with(model: @client_user, local: true) do |form| %>
                  <div class="form-group">
          <%= form.label :nome %>
          <%= form.text_field :nome, id: :nome ,class:"form-control", readonly:true%>
        </div>
                        <div class="form-group">
          <%= form.label :telefone %>
          <%= form.text_field :telefone, id: :telefone ,class:"form-control", readonly:true%>
        </div>
                        <div class="form-group">
          <%= form.label :cpf %>
          <%= form.text_field :cpf, id: :cpf ,class:"form-control", readonly:true%>
        </div>
                        <div class="form-group">
          <%= form.label :email %>
          <%= form.text_field :email, id: :email ,class:"form-control", readonly:true%>
        </div>
                        <div class="form-group">
          <%= form.label :password_digest %>
          <%= form.text_field :password_digest, id: :password_digest ,class:"form-control", readonly:true%>
        </div>
            <% end %>
  <%= link_to 'Editar', edit_client_user_path(@client_user),class:"btn btn-primary min-width-btn" %> 
  <%= link_to 'Voltar', client_users_path,class:"btn btn-light min-width-btn" %>
</div>