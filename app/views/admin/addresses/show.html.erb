<div class="container">
  <h1>Detalhes</h1>
  <%= form_with(model: @address, local: true, url: admin_administrator_addresses_path(@administrator, @address) ) do |form| %>
        <div class="form-group">
          <%= form.label :rua %>
          <%= form.text_field :rua, id: :rua ,class:"form-control", readonly:true%>
        </div>
         <div class="form-group">
          <%= form.label :numero %>
          <%= form.text_field :numero, id: :numero ,class:"form-control", readonly:true%>
        </div>
        <div class="form-group">
          <%= form.label :bairro %>
          <%= form.text_field :bairro, id: :bairro ,class:"form-control", readonly:true%>
        </div>
        <div class="form-group">
          <%= form.label :cep %>
          <%= form.text_field :cep, id: :cep ,class:"form-control", readonly:true%>
        </div>
        <div class="form-group">
          <%= form.label :estado %>
          <%= form.text_field :estado, id: :estado ,class:"form-control", readonly:true%>
        </div>
   <% end %>
  <%= link_to 'Editar', edit_admin_administrator_address_path(@administrator, @address),class:"btn btn-primary min-width-btn"  %>    
  <%= link_to 'Voltar', admin_administrator_addresses_path(@administrator),class:"btn btn-light min-width-btn" %>
</div>