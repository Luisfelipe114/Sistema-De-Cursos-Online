<div class="container">
  <h1>Detalhes</h1>
  <%= form_with(model: @bank_account, local: true, url: admin_administrator_bank_accounts_path(@administrator, @address)) do |form| %>
                  <div class="form-group">
          <%= form.label :numero %>
          <%= form.number_field :numero, id: :numero ,class:"form-control", readonly:true%>
        </div>
                        <div class="form-group">
          <%= form.label :agencia %>
          <%= form.number_field :agencia, id: :agencia ,class:"form-control", readonly:true%>
        </div>
                        <div class="form-group">
          <%= form.label :banco %>
          <%= form.text_field :banco, id: :banco ,class:"form-control", readonly:true%>
        </div>
            <% end %>
  <%= link_to 'Editar', edit_admin_administrator_bank_account_path(@administrator, @bank_account),class:"btn btn-primary min-width-btn" %> 
  <%= link_to 'Voltar', admin_administrator_bank_accounts_path(@administrator), class:"btn btn-light min-width-btn" %>
</div>